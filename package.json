{
  "name": "branchformat",
  "version": "1.0.7",
  "author": "Souler Ou(ou@souler.cc)",
  "description": "⚙ A simple util to help quick checkout or switch branches that correspond to team regulations",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "repository": {
    "url": "https://github.com/a20185/branchformat",
    "type": "git"
  },
  "scripts": {
    "test": "echo 'No test!'",
    "build": "rm -rf ./dist && rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "checkout": "node bin/checkout"
  },
  "bin": {
    "bfinit": "./postinstall.js",
    "bfeject": "./eject.js"
  },
  "files": [
    "README.md",
    "package.json",
    "postinstall.js",
    "eject.js",
    "branchformat.config.js",
    "babel-config.js",
    "assets",
    "dist"
  ],
  "keywords": [
    "branch",
    "lint",
    "format",
    "util",
    "git-checkout",
    "branch-switch",
    "standard",
    "regulation"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "3.0.0",
    "core-js": "^3.8.0",
    "inquirer": "^7.3.3",
    "rcfile": "^1.0.3",
    "semver": "7.0.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/semver": "6.22.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "commitizen": "4.2.1",
    "cz-customizable": "5.2.0",
    "jest": "^26.6.3",
    "rollup": "^2.23.0",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2"
  },
  "config": {
    "changelog": "降级 Chalk 与 Semver 以兼容 Node 8",
    "enchangelog": "Downgrade chalk and semver for compatible with node 8",
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-configrc.js"
    }
  }
}
