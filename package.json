{
  "name": "@nibfe/branchformat",
  "version": "0.0.17",
  "description": "⚙ A simple util to help quick checkout or switch branches that correspond to team regulations",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "echo 'No test!'",
    "build": "rm -rf ./dist && rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "checkout": "node bin/checkout"
  },
  "bin": {
    "bfinit": "./postinstall.js"
  },
  "files": [
    "package.json",
    "postinstall.js",
    "babel-config.js",
    "test.js",
    "dist",
    "src/templates"
  ],
  "keywords": [
    "branch",
    "lint",
    "format",
    "util",
    "git-checkout",
    "branch-switch",
    "standard",
    "regulation"
  ],
  "author": "Souler Ou",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "core-js": "^3.8.0",
    "inquirer": "^7.3.3",
    "rcfile": "^1.0.3",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "commitizen": "4.2.1",
    "cz-customizable": "5.2.0",
    "jest": "^26.6.3",
    "rollup": "^2.23.0",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2"
  },
  "config": {
    "changelogs": {
      "0.0.17": "减少版本更新检测的次数，默认设置为每使用 20 次后才进行检测尝试",
      "0.0.16": "修复 branchverify 验证分支不合法后不终止进程的问题"
    },
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-configrc.js"
    }
  }
}
